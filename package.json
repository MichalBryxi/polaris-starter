    "lint:js:fix": "eslint . --fix",
    "lint:prettier": "prettier . --check",
    "lint:prettier:fix": "prettier . --write",
    "lint:types": "glint",
    "start": "vite",
    "test": "vite build --mode test && ember test --path dist",
    "test:ember": "vite build --mode test && ember test --path dist"
  },
